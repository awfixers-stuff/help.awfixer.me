{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "AJ Services Help",
  "colors": {
    "primary": "#71797E",
    "light": "#71797E",
    "dark": "#71797E"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Help Center",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "services", "development"]
          },
          {
            "group": "Customization",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "Legal",
        "groups": [
          {
            "group": "Agreeements",
            "pages": ["legal/spa", "legal/sla"]
          },
          {
            "group": "Notices",
            "pages": [
              "legal/privacy",
              "legal/subprocessors",
              "legal/terms",
              "legal/usage"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "AWFixer and Friends",
          "href": "https://awfixer.com",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.awfixer.me",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://awfixer.blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "https://github.com/awfixers-stuff.png",
    "dark": "https://github.com/awfixers-stuff.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.awfixer.me"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://my.awfixer.me"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/awfixer",
      "github": "https://github.com/awfixers-stuff",
      "discord": "https://discord.awfixer.me"
    }
  }
}
