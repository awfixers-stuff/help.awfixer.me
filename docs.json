          {
            "group": "Overview",
            "pages": ["services/services"]
          },
          {
            "group": "Standard",
            "pages": [
              "services/idm",
              "services/proxy",
              "services/tor",
              "services/vpn"
            ]
          },
          {
            "group": "Premium",
            "pages": ["services/premium/premium"]
          }
        ]
      },
      {
        "tab": "Products",
        "groups": [
          {
            "group": "Bots",
            "icon": "discord",
            "pages": ["products/firewall", "products/support-manager"]
          },
          {
            "group": "Firewall Bot",
            "icon": "block-brick",
            "pages": [
              "products/firewall/overview",
              "products/firewall/silent",
              "products/firewall/premium"
            ]
          }
        ]
      },
