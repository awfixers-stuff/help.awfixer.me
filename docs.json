        ]
      },
      {
        "tab": "information",
        "groups": [
          {
            "group": "Premium",
            "pages": ["info/donorperks"]
          },
          {
            "group": "Community",
            "pages": ["info/community", "info/level", "info/helper"]
          }
        ]
      }
