{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "AJ Services Help",
  "banner": {
    "content": "be sure to join the [Discord](https://discord.awfixer.me) for the latest updates",
    "dismissible": true
  },
  "colors": {
    "primary": "#ffffff",
    "light": "#d3d3d3",
    "dark": "#000000"
  },
  "favicon": "/favicon.png",
  "errors": {
    "404": {
      "redirect": false,
      "title": "I can't be found...",
      "description": "whatever happened to this __page__?"
